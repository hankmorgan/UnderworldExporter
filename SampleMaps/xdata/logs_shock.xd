readables/shock/log_2540
	{
	precache
	"num_pages" : "1"
	"page1_title" :
		{
		"Harvey-18.OCT.72\n"
		}
	"page1_body" :
		{
		"Sender: Jason Harvey\nSHODAN's virus\nIt'll be over soon... soon I'll die. This painful end... I can't even recognize my own damn arm. SHODAN's virus.\nI can see... oh God, I can see through my own body! There's another virus in the grove... God, let me die... please.\n\n\n\n"
		}
		
	"gui_page1"	: "guis/readables/log.gui"
	"page1_PortraitLeft" : "guis/assets/shock/logs/0040_0028.tga"
	"page1_PortraitRight" : "guis/assets/shock/logs/0040_0030.tga"
}

