readables/shock/log_2557
	{
	precache
	"num_pages" : "1"
	"page1_title" :
		{
		"Aubry-06.JUN.72\n"
		}
	"page1_body" :
		{
		"Sender: Ed Aubry\nnew moving schedule\nSabo, you've got to be kidding me! Is this work schedule some kind of joke? Two six hour shifts a day with a 45 minute break in between.\nMight as well just take the lifter-bots offline so I can do it all by hand!\n\n\n\n"
		}
		
	"gui_page1"	: "guis/readables/log.gui"
	"page1_PortraitLeft" : "guis/assets/shock/logs/0040_0026.tga"
